    <rule-file/>
    <rule-definition>package org.pentaho.di.trans.steps.rules
 
import org.pentaho.di.trans.steps.rules.Rules.Column;

global java.util.List list
 
rule "Get first name"
    dialect "mvel"
	when
		Column( name == "nameFirst", $nameFirst : payload)
	then
	    System.out.println( $nameFirst );
end

rule "Get last name"
    dialect "mvel"
	when
		Column( name == "nameLast", $nameLast : payload)
	then
	    System.out.println( $nameLast );
end

rule "Compile name"
    dialect "mvel"
    when
        $nameFirst : Column(name == "nameFirst")
        $nameLast : Column(name == "nameLast")
    then
        System.out.println("User: " + $nameFirst.payload + " " + $nameLast.payload);
end

rule "Decline"
    dialect "mvel"
    when
        $fica : Column(name == "fica", payload &lt; 550)
        
    then
        Column approvalStatus = new Column();
	Column approvalClassification = new Column();

        insert(approvalStatus);
        insert(approvalClassification);

        approvalStatus.name = "approvalStatus"
        approvalStatus.type = String.class
        approvalStatus.payload = "declined"
        
        
        approvalClassification.name = "approvalClass"
        approvalClassification.type = String.class
        approvalClassification.payload = "Declined: fica too low"
        
        
        System.out.println("Declined");
end

rule "Further investigation required"
    dialect "mvel"
    when
        not (exists Column(name == "fica"))
        
    then
        Column approvalStatus = new Column();
        approvalStatus.name = "approvalStatus"
        approvalStatus.type = String.class
        approvalStatus.payload = "investigate"
        
        Column approvalClassification = new Column();
        approvalClassification.name = "approvalClass"
        approvalClassification.type = String.class
        approvalClassification.payload = "Investigate: no fica found"
        
        insert(approvalStatus);
        insert(approvalClassification);
        
        System.out.println("Investigate");
end

rule "Approve class C"
    dialect "mvel"
    when
        $fica : Column(name == "fica", payload >= 550, payload &lt; 610)
        
    then
        Column approvalStatus = new Column();
        approvalStatus.name = "approvalStatus"
        approvalStatus.type = String.class
        approvalStatus.payload = "approved"
        
        Column approvalClassification = new Column();
        approvalClassification.name = "approvalClass"
        approvalClassification.type = String.class
        approvalClassification.payload = "C"
        
        insert(approvalStatus);
        insert(approvalClassification);
        
        System.out.println("Approved");
end

rule "Approve class B"
    dialect "mvel"
    when
        $fica : Column(name == "fica", payload >= 610, payload &lt; 695)
        
    then
        Column approvalStatus = new Column();
        approvalStatus.name = "approvalStatus"
        approvalStatus.type = String.class
        approvalStatus.payload = "approved"
        
        Column approvalClassification = new Column();
        approvalClassification.name = "approvalClass"
        approvalClassification.type = String.class
        approvalClassification.payload = "B"
        
        insert(approvalStatus);
        insert(approvalClassification);
        
        System.out.println("Approved");
end

rule "Approve class A"
    dialect "mvel"
    when
        $fica : Column(name == "fica", payload >= 695)
        
    then
        Column approvalStatus = new Column();
        approvalStatus.name = "approvalStatus"
        approvalStatus.type = String.class
        approvalStatus.payload = "approved"
        
        Column approvalClassification = new Column();
        approvalClassification.name = "approvalClass"
        approvalClassification.type = String.class
        approvalClassification.payload = "A"
        
        insert(approvalStatus);
        insert(approvalClassification);
        
        System.out.println("Approved");
end</rule-definition>
